<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.leolee.msf.dao.TestMapper">

    <select id="selectAll" resultType="com.leolee.msf.entity.TestEntity">
        select * from test
    </select>


    <select id="selectByName" resultType="com.leolee.msf.entity.TestEntity">
        select * from test where `name` = #{name}
    </select>
</mapper>
